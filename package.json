    "react-easy-crop": "^5.5.0",
    "redis": "^5.8.2",
    "stockfish": "^17.1.0",
    "three": "^0.180.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "http-proxy-middleware": "^3.0.5",
    "localtunnel": "^2.0.2",
    "qrcode-terminal": "^0.12.0"
  }
